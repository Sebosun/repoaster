        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.18",
        "@types/multer": "^1.4.8",
        "@types/node": "^20.8.4",
        "eslint": "^9.23.0",
        "eslint-plugin-unicorn": "^58.0.0",
        "globals": "^16.0.0",
        "ts-node": "^10.9.1",
        "tsx": "^3.12.7",
        "typescript": "^5.1.6",
        "typescript-eslint": "^8.29.0"
